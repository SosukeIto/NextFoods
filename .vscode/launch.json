{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Next.js: Full Stack",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
          "run",
          "dev"
        ],
        "skipFiles": [
          "<node_internals>/**"
        ],
        "sourceMaps": true
      },
      {
        "name": "Next.js: Client-side",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}"
      }
    ]
  }
  